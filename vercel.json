{
  "crons": [
    {
      "path": "/api/build",
      "schedule": "0 1 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*).(png|svg|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
