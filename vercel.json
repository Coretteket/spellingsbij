{
  "crons": [
    {
      "path": "/api/build",
      "schedule": "0 2 * * *"
    }
  ],
  "headers": [
    {
      "source": "/**/*.{svg,png,woff2}",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
